{
  "name": "upwe-app",
  "version": "2.0.0",
  "description": "UpWe - A professional skill exchange platform where learning meets earning. Share expertise, discover talents, and grow with a global community of learners and teachers.",
  "main": "index.js",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "UpWe Team",
    "email": "info@upwe.example.com",
    "url": "https://upwe.example.com"
  },
  "contributors": [
    {
      "name": "Development Team",
      "email": "dev@upwe.example.com"
    }
  ],
  "scripts": {
    "start": "live-server src --port=8000",
    "dev": "live-server src",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "serve": "python -m http.server 8000 --directory src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,html,json}\"",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "clean": "rm -rf dist node_modules",
    "reinstall": "npm run clean && npm install"
  },
  "keywords": [
    "skill",
    "exchange",
    "learning",
    "teaching",
    "platform",
    "community",
    "education",
    "upwe",
    "social-learning",
    "peer-to-peer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/upwe/upwe-app.git"
  },
  "bugs": {
    "url": "https://github.com/upwe/upwe-app/issues",
    "email": "support@upwe.example.com"
  },
  "homepage": "https://upwe.example.com",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "firebase": "^10.7.1",
    "firebase-admin": "^12.0.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "chart.js": "^4.4.0",
    "socket.io-client": "^4.7.2",
    "eventemitter2": "^6.4.9"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-bundle-analyzer": "^4.10.1",
    "live-server": "^1.2.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "node-sass": "^9.0.0",
    "sass-loader": "^13.3.2",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "html-webpack-plugin": "^5.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "terser-webpack-plugin": "^5.3.9",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.4",
    "autoprefixer": "^10.4.16"
  }
}